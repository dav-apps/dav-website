{
	"name": "dav-website",
	"version": "1.7.5",
	"scripts": {
		"ng": "ng",
		"start": "cd dist && node server.js",
		"build": "npm run build:prod",
		"build:watch": "ng build --watch",
		"build:prod": "npm run clear && npm run npm-install && npm run ng-build-prod && npm run server-compile && npm run server-copy",
		"clear": "rm -rf dist && rm -rf server/dist",
		"npm-install": "npm install && npm --prefix ./server/ install ./server/",
		"ng-build-prod": "ng build --configuration production",
		"server-compile": "( cd server/ ; tsc )",
		"server-copy": "cpx 'server/{package.json,package-lock.json,server.js,web.config,node_modules/**/*}' dist/ && cpx 'server/dist/**/*' dist/",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~12.2.5",
		"@angular/cdk": "~12.2.5",
		"@angular/common": "~12.2.5",
		"@angular/compiler": "~12.2.5",
		"@angular/core": "~12.2.5",
		"@angular/forms": "~12.2.5",
		"@angular/localize": "^12.2.5",
		"@angular/material": "^12.2.5",
		"@angular/platform-browser": "~12.2.5",
		"@angular/platform-browser-dynamic": "~12.2.5",
		"@angular/router": "~12.2.5",
		"@angular/service-worker": "~12.2.5",
		"@fluentui/web-components": "^2.1.1",
		"@fortawesome/angular-fontawesome": "^0.9.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.36",
		"@fortawesome/free-brands-svg-icons": "^5.15.4",
		"@fortawesome/free-solid-svg-icons": "^5.15.4",
		"@schematics/angular": "^9.0.3",
		"bootstrap": "^5.1.3",
		"chart.js": "^2.9.3",
		"chartkick": "^3.2.0",
		"dav-js": "git+https://github.com/dav-apps/dav-js.git#v2.1.1",
		"dav-ui-components": "git+https://github.com/dav-apps/dav-ui-components.git#v0.7.6",
		"moment": "2.24.0",
		"ng2-charts": "^2.3.0",
		"ngx-file-helpers": "^6.0.0",
		"office-ui-fabric-core": "^11.0.0",
		"rxjs": "^6.6.7",
		"socket.io": "^4.2.0",
		"stripe": "^8.22.0",
		"tslib": "^2.0.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^12.2.5",
		"@angular-devkit/core": "^12.2.5",
		"@angular-devkit/schematics": "^12.2.5",
		"@angular/cli": "~12.2.5",
		"@angular/compiler-cli": "~12.2.5",
		"@angular/language-service": "~12.2.5",
		"@types/jasmine": "~3.6.0",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^12.11.1",
		"@types/react": "^16.9.23",
		"@types/react-dom": "^16.9.5",
		"codelyzer": "^6.0.0",
		"cpx": "^1.5.0",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~6.3.4",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"protractor": "~7.0.0",
		"ts-node": "~7.0.0",
		"tslint": "~6.1.0",
		"typescript": "~4.3.5"
	},
	"engines": {
		"node": "14.x",
		"npm": "8.x"
	}
}