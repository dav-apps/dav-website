{
  "name": "dav-website",
  "version": "1.3.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:watch": "ng build --watch",
    "build:prod": "npm run clear && npm run npm-install && npm run ng-build-prod && npm run server-compile && npm run server-copy",
    "clear": "rm -rf dist && rm -rf server/dist",
    "npm-install": "npm install && npm --prefix ./server/ install ./server/",
    "ng-build-prod": "ng build --prod --aot",
    "server-compile": "( cd server/ ; tsc )",
    "server-copy": "cpx 'server/{package.json,package-lock.json,server.js,web.config,node_modules/**/*}' dist/ && cpx 'server/dist/**/*' dist/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-react/core": "^1.0.1",
    "@angular-react/fabric": "^1.2.3",
    "@angular/animations": "~9.0.2",
    "@angular/cdk": "~9.1.0",
    "@angular/common": "~9.0.2",
    "@angular/compiler": "~9.0.2",
    "@angular/core": "~9.0.2",
    "@angular/forms": "~9.0.2",
    "@angular/localize": "^9.0.2",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "~9.0.2",
    "@angular/platform-browser-dynamic": "~9.0.2",
    "@angular/router": "~9.0.2",
    "@angular/service-worker": "~9.0.2",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@ng-bootstrap/ng-bootstrap": "^5.3.0",
    "@schematics/angular": "^9.0.3",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "chartkick": "^3.2.0",
    "dav-npm": "git+https://github.com/dav-apps/dav-npm.git#v1.4",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "ng2-charts": "^2.3.0",
    "ngx-file-helpers": "^2.0.0",
    "office-ui-fabric-core": "^10.1.0",
    "office-ui-fabric-react": "^6.211.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rxjs": "~6.5.4",
    "socket.io": "^2.3.0",
    "stripe": "^8.22.0",
    "tslib": "^1.11.0",
    "zone.js": "~0.10.2"
