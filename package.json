{
  "name": "dav-website",
  "version": "1.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:watch": "ng build --watch",
    "build:prod": "npm run clear && npm run npm-install && npm run ng-build-prod && npm run server-compile && npm run server-copy",
    "clear": "rm -rf dist && rm -rf server/dist",
    "npm-install": "npm install && npm --prefix ./server/ install ./server/",
    "ng-build-prod": "ng build --prod --aot",
    "server-compile": "( cd server/ ; tsc )",
    "server-copy": "cpx 'server/{package.json,package-lock.json,server.js,web.config,node_modules/**/*}' dist/ && cpx 'server/dist/**/*' dist/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-react/core": "^1.0.1",
    "@angular-react/fabric": "^1.2.1",
    "@angular/animations": "~8.2.7",
    "@angular/cdk": "~8.2.1",
    "@angular/common": "~8.2.7",
    "@angular/compiler": "~8.2.7",
    "@angular/core": "~8.2.7",
    "@angular/forms": "~8.2.7",
    "@angular/material": "^8.2.1",
    "@angular/platform-browser": "~8.2.7",
    "@angular/platform-browser-dynamic": "~8.2.7",
    "@angular/pwa": "^0.803.5",
    "@angular/router": "~8.2.7",
    "@angular/service-worker": "~8.2.7",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@ng-bootstrap/ng-bootstrap": "^5.1.1",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.9.2",
    "chartkick": "^3.2.0",
    "dav-npm": "git+https://github.com/dav-apps/dav-npm.git#v1.3",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "ng2-charts": "^2.3.0",
    "ngx-file-helpers": "^2.0.0",
    "office-ui-fabric-core": "^10.1.0",
    "office-ui-fabric-react": "^6.151.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rxjs": "~6.4.0",
    "socket.io": "^2.3.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.5",
